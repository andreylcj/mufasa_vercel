n  }\r\n\r\n  const updateInfor = async () => {\r\n   \r\n    dispatch({type: 'NOTIFY', payload: {loading: true}})\r\n\r\n    \r\n\r\n    putData('user', {\r\n        CPF , CEICEIpassword\r\n    }, auth.token).then(res => {\r\n        if(res.err) return dispatch({type: 'NOTIFY', payload: {error: res.err}})\r\n\r\n        dispatch({type: 'AUTH', payload: {\r\n            token: auth.token,\r\n            user: res.user\r\n        }})\r\n        return dispatch({type: 'NOTIFY', payload: {success: res.msg}})\r\n    })\r\n}\r\n\r\n  const handleInputChange = (e) => {\r\n    const { name, value } = e.target;\r\n    setSubmitData({\r\n      ...submitData,\r\n      [name]: value,\r\n    });\r\n  };\r\n\r\n  const handleSubmit = async (e) => {\r\n    e.preventDefault();\r\n\r\n    const validData = await validCPFAuth(submitData.CPF, submitData.CEIpassword);\r\n\r\n    if (!validData.validate) {\r\n      setSubmitData({\r\n        ...submitData,\r\n        CPFErr: validData.CPFMsg,\r\n        CEIpasswordMsg: validData.CEIpasswordMsg,\r\n      });\r\n      console.log(validData);\r\n      return;\r\n    }\r\n\r\n    setSubmitData({\r\n      ...submitData,\r\n      CPFErr: '',\r\n      CEIpasswordMsg: '',\r\n    });\r\n\r\n    const CEIdata = {\r\n      CPF: submitData.CPF.split('.').join('').split('-').join(''),\r\n      CEIpassword: submitData.CEIpassword,\r\n      _id: submitData.user_id,\r\n    };\r\n\r\n    console.log(CEIdata);\r\n\r\n    dispatch({ type: 'START_LOADING' });\r\n    // save on mongo db\r\n    dispatch({ type: 'END_LOADING' });\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (!auth.user) return;\r\n\r\n    setSubmitData({\r\n      ...submitData,\r\n      user_id: auth.user._id,\r\n    });\r\n  }, [auth.user]);\r\n\r\n  return (\r\n    <>\r\n      <GoBackButton />\r\n\r\n      <CEI className=\"container\">\r\n        <CEI.Header>\r\n          CEI INFO\r\n        </CEI.Header>\r\n\r\n        <CEI.Form\r\n          onSubmit={handleSubmit}\r\n          noValidate\r\n          className=\"mx-auto my-4\"\r\n          style={{ maxWidth: '500px' }}\r\n        >\r\n\r\n          <div className=\"mb-3\">\r\n            <label htmlFor=\"CPF\" className=\"form-label\">CPF</label>\r\n            <input\r\n              type=\"text\"\r\n              className=\"form-control\"\r\n              id=\"CPF\"\r\n              aria-describedby=\"emailHelp\"\r\n              name=\"CPF\"\r\n              onChange={handleInputChange}\r\n              value={submitData.CPF}\r\n            />\r\n            <div id=\"CPFHelp\" className={`form-text${submitData.CPFErr ? ' text-danger' : ''}`}>\r\n              {\r\n                                submitData.CPFErr\r\n                                  ? (\r\n                                    <p>{submitData.CPFErr}</p>\r\n                                  ) : (\r\n                                    null\r\n                                  )\r\n                            }\r\n            </div>\r\n          </div>\r\n          <div className=\"mb-3\">\r\n            <label htmlFor=\"CEIpassword\" className=\"form-label\">Senha</label>\r\n            <input\r\n              type=\"CEIpassword\"\r\n              className=\"form-control\"\r\n              id=\"CEIpassword\"\r\n              name=\"CEIpassword\"\r\n              onChange={handleInputChange}\r\n              value={submitData.CEIpassword}\r\n            />\r\n            <div id=\"CEIpasswordHelp\" className={`form-text${submitData.CEIpasswordMsg ? ' text-danger' : ''}`}>\r\n              {\r\n                                submitData.CEIpasswordMsg\r\n                                  ? (\r\n                                    <p>{submitData.CEIpasswordMsg}</p>\r\n                                  ) : (\r\n                                    'Nós nunca vamos compartilhar seus dados com ninguém.'\r\n                                  )\r\n                            }\r\n            </div>\r\n          </div>\r\n          {/*\r\n                    <div className=\"mb-3 form-check\">\r\n                    <input type=\"checkbox\" className=\"form-check-input\" id=\"checkMeOut\" />\r\n                    <label className=\"form-check-label\" htmlFor=\"checkMeOut\">Lembrar de mim</label>\r\n                </div>\r\n                */}\r\n          <button type=\"submit\" className=\"btn btn-primary d-flex mx-auto\" onClick={handleUpdateProfile}>\r\n            {\r\n                            loading\r\n                              ? (\r\n                                <>\r\n                                  <Loading />\r\n                                </>\r\n                              ) : (\r\n                                'Salvar'\r\n                              )\r\n                        }\r\n          </button>\r\n        </CEI.Form>\r\n      </CEI>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default insertCEIinfo;\r\n"]},"metadata":{},"sourceType":"module"}